{
  "name": "zk/laravel-formbuilder",
  "license": "MIT",
  "description": "A powerful and flexible form-building solution for Laravel, featuring Blade components, lazy loading, and a custom JavaScript library for client-side enhancements.",
  "keywords": [
    "Laravel form builder",
    "Laravel dynamic forms",
    "JavaScript form builder",
    "Laravel custom form components",
    "Laravel lazy loading forms",
    "Client-side form validation",
    "Form builder for Laravel",
    "Laravel UI components",
    "Laravel frontend validation"
  ],
  "authors": [
    {
      "name": "Zankat Kalpesh",
      "email": "zkdeveloper24@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2|^8.3",
    "laravel/framework": "^11.0|^12.0"
  },
  "autoload": {
    "psr-4": {
      "Zk\\FormBuilder\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Zk\\FormBuilder\\Providers\\FormBuilderServiceProvider"
      ],
      "aliases": {}
    }
  },
  "minimum-stability": "dev"
}
